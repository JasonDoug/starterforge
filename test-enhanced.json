{
  "project_type": "web_app",
  "frontend": {
    "framework": "react",
    "ui_libraries": ["tailwindcss"]
  },
  "backend": {
    "language": "node",
    "framework": "express"
  },
  "database": {
    "engines": ["postgresql"],
    "orm": "prisma",
    "models": ["post", "comment"]
  },
  "auth": {
    "provider": "auth0",
    "include_ui": true
  },
  "output": {
    "format": ["zip"]
  }
}